#include<iostream>
using namespace std;

int main() {
    int year,leap;
    cout << "Please enter year:";
    cin >> year;
    if((year % 4 == 0 && year %100 != 0)|| year % 400 ==0) {
        leap = 1;
    } else {
        leap = 0;    
    }
    if (leap) {
		cout << year << "is a leap year" << endl;
	}else {
		cout << year << "is not a leap year" << endl;
	}
	return 0;
}
